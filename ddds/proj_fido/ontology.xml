<?xml version="1.0" encoding="utf-8"?>
<ontology name="ProjFidoOntology">

  <sort name="dog" dynamic="true"/>
  <sort name="p_score"/>
  <sort name="description" dynamic="true"/>
  <sort name="comparison" dynamic="true"/>
  <sort name="feature"/>

  <sort name="explanation"/>
  
  <action name="get_dog"/>

  <action name="solve_zero"/>

  <action name="solve_overflow"/>

  <!-- (Dis)Preferred Features (pf) for incremental search-->
  <predicate name="pf_trainability" sort="p_score"/>
  <predicate name="pf_shedding" sort="p_score"/>
  <predicate name="pf_energy" sort="p_score"/>
  <predicate name="pf_barking" sort="p_score"/>
  <predicate name="pf_protectiveness" sort="p_score"/>


  <predicate name="what_dog_to_get" sort="dog"/>
  
  
  <predicate name="describe_dog" sort="description"/>
  <predicate name="what_dog_to_describe" sort="dog"/>


  <predicate name="compare" sort="comparison"/>
  <predicate name="target_dog" sort="dog"/>
  <predicate name="compare_with" sort="dog"/>
  <predicate name="feature_of_comparison" sort="feature"/>
  <predicate name="most_dog" sort="dog"/>
  <predicate name="feature_of_most" sort="feature"/>
  <predicate name="least_dog" sort="dog"/>
  <predicate name="feature_of_least" sort="feature"/>

  <predicate name="explain_no_hits" sort="explanation"/>
  <predicate name="explain_too_many_hits" sort="explanation"/>
  <predicate name="explain_procedure" sort="explanation"/>


  <individual name="zero" sort="p_score"/>
  <individual name="one" sort="p_score"/>
  <individual name="two" sort="p_score"/>
  <individual name="three" sort="p_score"/>
  <individual name="four" sort="p_score"/>
  <individual name="five" sort="p_score"/>

  <individual name="trainability" sort="feature"/>
  <individual name="shedding" sort="feature"/>  
  <individual name="energy" sort="feature"/>
  <individual name="barking" sort="feature"/>
  <individual name="protectiveness" sort="feature"/>
  <individual name="good_with_children" sort="feature"/>   


  <individual name="procedure" sort="explanation"/>
  <individual name="no_hits" sort="explanation"/>
  <individual name="too_many_hits" sort="explanation"/>

</ontology>
